<% layout('layouts/boilerplate') %>
    <div class="row">
        <div class="col-6 offset-3">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">
                    <%= err.message %>
                </h4>
                <div class="text-break">
                    <p>
                        <%= err.stack %>
                    </p>
                </div>

                <hr>

                <div class="image-error"></div>
            </div>
        </div>
    </div>